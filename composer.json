{
    "name": "meritoo/menu-bundle",
    "description": "Build navigation easily, without any efforts. Bundle based on Symfony framework.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Meritoo.pl",
            "email": "github@meritoo.pl"
        }
    ],
    "require": {
        "php": "^7.2",
        "meritoo/common-bundle": "^0.1.22",
        "meritoo/menu": "^0.0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "infection/infection": "^0.11.4",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.11.1",
        "phpunit/phpunit": "^8.0",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^2.9",
        "symfony/phpunit-bridge": "^4.1",
        "vimeo/psalm": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Meritoo\\MenuBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meritoo\\Test\\MenuBundle\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
